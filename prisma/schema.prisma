// Looking for ways to speed up your queries, or scale easily with your serverless or edge functions?
// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Picture {
  id    String     @default(uuid()) @id
  picture Bytes?
  name String? @db.VarChar(500)
  type    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  newsItem NewsItem[]
  articleItem ArticleItem[]
  portfolio Portfolio[]
  news News[]
  article Article[]
  categoryArea CategoryArea[]
  builds Build[]
  feedback Feedback[]
  category Category[]
  previewPicture Category[] @relation("PreviewPicture")
  buildIconPictures Build[] @relation("BuildIconPicture")
  categoryIconPictures Category[] @relation("CategoryIconPicture")
  @@map("pictures")
}

model Video {
  id    String     @default(uuid()) @id
  originalname String? 
  filename String?
  path String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  feedback Feedback[]
  category Category[]
  news News[]
  article Article[]
  @@map("videos")
}

model News {
  id    String     @default(uuid()) @id
  number      Int        @default(autoincrement())
  title     String? @db.VarChar(500)
  subtitle     String? @db.VarChar(500)
  picture     Picture?   @relation(fields: [pictureId], references: [id])
  pictureId   String?    @default(uuid())
  time String?
  video    Video?    @relation(fields: [videoId], references: [id])
  videoId  String? @default(uuid())
  list        Json?
  status    ContentSatus?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  newsItem NewsItem[]
  @@map("news")
}

model Article {
  id    String     @default(uuid()) @id
  number      Int        @default(autoincrement())
  title     String? @db.VarChar(500)
  subtitle     String? @db.VarChar(500)
  picture     Picture?   @relation(fields: [pictureId], references: [id])
  pictureId   String?    @default(uuid())
  time String?
  video    Video?    @relation(fields: [videoId], references: [id])
  videoId  String? @default(uuid())
  list        Json?
  status    ContentSatus?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  articleItem ArticleItem[]
  @@map("articles")
}

model NewsItem {
  id          String     @default(uuid()) @id
  text        String?    @db.VarChar(3000)
  picture     Picture?   @relation(fields: [pictureId], references: [id])
  pictureId   String?    @default(uuid())
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  news     News?   @relation(fields: [newsId], references: [id])
  newsId String?    @default(uuid())
  list        Json?
  @@map("news_items")
}

model ArticleItem {
  id          String     @default(uuid()) @id
  text        String?    @db.VarChar(3000)
  picture     Picture?   @relation(fields: [pictureId], references: [id])
  pictureId   String?    @default(uuid())
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  article     Article?   @relation(fields: [articleId], references: [id])
  articleId String?    @default(uuid())
  list        Json?
  @@map("article_items")
}

model Portfolio {
  id    String     @default(uuid()) @id
  number      Int        @default(autoincrement())
  title     String? @db.VarChar(500)
  description   String? @db.VarChar(3000)
  categoryName String?
  category    Category?    @relation(fields: [categoryId], references: [id])
  categoryId  String? @default(uuid())
  picture    Picture?    @relation(fields: [pictureId], references: [id])
  pictureId  String? @default(uuid())
  status    ContentSatus?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("portfolios")
}

model Section {
  id                String        @default(uuid()) @id
  number            Int           @default(autoincrement())
  title             String?       @db.VarChar(500)
  description       String?       @db.VarChar(3000)
  status            ContentSatus?
  createdAt         DateTime      @default(now())
  updatedAt         DateTime      @updatedAt
  ategory Category[]
  @@map("sections")
}

model Category {
  id    String     @default(uuid()) @id
  number            Int           @default(autoincrement())
  title     String? @db.VarChar(500)
  description   String? @db.VarChar(3000)
  subtitle     String? @db.VarChar(500)
  section    Section?    @relation(fields: [sectionId], references: [id])
  sectionId  String? @default(uuid())
  picture    Picture?    @relation(fields: [pictureId], references: [id])
  pictureId  String? @default(uuid())
  video    Video?    @relation(fields: [videoId], references: [id])
  previewPicture Picture? @relation("PreviewPicture", fields: [previewPictureId], references: [id])
  previewPictureId String? @default(uuid())
  iconPicture Picture? @relation("CategoryIconPicture", fields: [iconPictureId], references: [id])
  iconPictureId String? @default(uuid())
  videoId  String? @default(uuid())
  list        Json?
  status            ContentSatus?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  portfolio Portfolio[]
  categoryArea CategoryArea[]
  @@map("categories")
}

model Area {
  id                String        @default(uuid()) @id
  number            Int           @default(autoincrement())
  lat               Float?
  lon               Float?
  name              String?       @db.VarChar(500)
  status            ContentSatus?
  createdAt         DateTime      @default(now())
  updatedAt         DateTime      @updatedAt
  categoryArea CategoryArea[]
  @@map("areas")
}

model CategoryArea {
  id                String        @default(uuid()) @id
  categoryId String  @default(uuid())
  areaId    String  @default(uuid())
  category   Category  @relation(fields: [categoryId], references: [id])
  area      Area     @relation(fields: [areaId], references: [id])
  title             String?       @db.VarChar(500)
  description       String?       @db.VarChar(3000)
  subTitle          String?       @db.VarChar(500)
  status    ContentSatus?
  picture     Picture?   @relation(fields: [pictureId], references: [id])
  pictureId   String?    @default(uuid())
  createdAt         DateTime      @default(now())
  updatedAt         DateTime      @updatedAt
  build             Build[]
  @@map("category_area")
}

model Build {
  id                String        @default(uuid()) @id
  number            Int           @default(autoincrement())
  coordinates       Json?
  name              String?       @db.VarChar(500)
  wDescription      String?       @db.VarChar(3000)
  picture           Picture?   @relation(fields: [pictureId], references: [id])
  pictureId         String?    @default(uuid())
  gTitle            String?       @db.VarChar(500)
  gSubTitle         String?       @db.VarChar(500)
  list              Json?
  status            ContentSatus?
  categoryAreaId     String?  @default(uuid())
  categoryArea       CategoryArea?     @relation(fields: [categoryAreaId], references: [id])
  iconPicture Picture? @relation("BuildIconPicture", fields: [iconPictureId], references: [id])
  iconPictureId String? @default(uuid())
  buildAreaCoordinates              Json?
  createdAt         DateTime      @default(now())
  updatedAt         DateTime      @updatedAt
  @@map("builds")
}

model Feedback {
  id    String     @default(uuid()) @id
  number      Int        @default(autoincrement())
  title     String? @db.VarChar(500)
  description   String? @db.VarChar(3000)
  video    Video?    @relation(fields: [videoId], references: [id])
  videoId  String? @default(uuid())
  picture    Picture?    @relation(fields: [pictureId], references: [id])
  pictureId  String? @default(uuid())
  status    ContentSatus?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("feedbacks")
}

enum ContentSatus {
  PUBLISHED
  DRAFT
  ARCHIVE
}

model Files {
  id    String     @default(uuid()) @id
  originalname String? 
  filename String?
  path String?
  type FileType?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("files")
}

enum FileType {
  TEMPLATE
  CURRENT
}
